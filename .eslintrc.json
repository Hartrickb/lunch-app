{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["plugin:react/recommended", "prettier", "plugin:unicorn/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "prettier", "unicorn"],
  "rules": {
    "no-console": "off",
    "react/react-in-jsx-scope": "off",
    "unicorn/filename-case": "off",
    "unicorn/consistent-function-scoping": "off"
    // "prettier/prettier": "error"
  }
}
